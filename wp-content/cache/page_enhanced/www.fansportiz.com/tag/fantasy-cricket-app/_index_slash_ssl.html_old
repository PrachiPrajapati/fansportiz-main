	var wowScript = document.createElement('script');
	wowScript.setAttribute('src','https://www.fansportiz.com/wp-content/themes/fansportiz-child/js/wow.min.js');
	document.head.appendChild(wowScript);
	wowScript.onload = function(){		
		new WOW().init();
	};
	var tiltScript = document.createElement('script');
	tiltScript.setAttribute('src','https://www.fansportiz.com/wp-content/themes/fansportiz-child/js/tilt.jquery.min.js');
	document.head.appendChild(tiltScript);
}

